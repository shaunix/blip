<?xml version="1.0" encoding="utf-8" ?>
<group>

<defaults>
  <module>
    <name>%(id)</name>
    <bug_server>http://bugzilla.gnome.org/</bug_server>
    <bug_product>%(id)</bug_product>
    <bug_component>general</bug_component>
    <rcs_server idref="gnome-cvs"/>
    <rcs_module>%(id)</rcs_module>
    <rcs_web>http://cvs.gnome.org/viewcvs/%(rcs_module)/</rcs_web>
    <download>http://ftp.gnome.org/pub/gnome/sources/%(id)/</download>
  </module>
  <branch>
    <bug_server>%(module/bug_server)</bug_server>
    <bug_product>%(module/bug_product)</bug_product>
    <bug_component>%(module/bug_component)</bug_component>
    <rcs_server idref="%(module/rcs_server)"/>
    <rcs_module>%(module/rcs_module)</rcs_module>
    <rcs_branch>%(id)</rcs_branch>
    <rcs_web>http://cvs.gnome.org/viewcvs/%(rcs_module)/?only_with_tag=%(rcs_branch)</rcs_web>
  </branch>
  <domain>
    <bug_server>%(branch/bug_server)</bug_server>
    <bug_product>l10n</bug_product>
    <rcs_server idref="%(branch/rcs_server)"/>
    <rcs_module>%(branch/rcs_module)</rcs_module>
    <rcs_branch>%(branch/rcs_branch)</rcs_branch>
    <rcs_dir>%(id)</rcs_dir>
    <rcs_web>http://cvs.gnome.org/viewcvs/%(rcs_module)/%(rcs_dir)/?only_with_tag=%(rcs_branch)</rcs_web>
  </domain>
  <document>
    <bug_server>%(branch/bug_server)</bug_server>
    <bug_product>%(branch/bug_product)</bug_product>
    <bug_component>docs</bug_component>
    <rcs_server idref="%(branch/rcs_server)"/>
    <rcs_module>%(branch/rcs_module)</rcs_module>
    <rcs_branch>%(branch/rcs_branch)</rcs_branch>
    <rcs_web>http://cvs.gnome.org/viewcvs/%(rcs_module)/%(rcs_dir)/?only_with_tag=%(rcs_branch)</rcs_web>
  </document>
</defaults>

<server id="gnome-cvs">
  <rcs_type>cvs</rcs_type>
  <rcs_root>:pserver:anonymous@anoncvs.gnome.org:/cvs/gnome</rcs_root>
</server>

