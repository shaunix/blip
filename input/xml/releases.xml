<release id="gnome-2-12">
  <name>Gnome 2.12</name>

  <defaults>
    <module><branch>gnome-2-14</branch></module>
  </defaults>

  <category id="platform">
    <name>Development Platform</name>
    <module id="gtk+"><branch>gtk-2-8</branch></module>
    <module id="glib"><branch>glib-2-8"</branch></module>
  </category>

  <category id="desktop">
    <name>Desktop Applications</name>
    <module id="epiphany"/>
  </category>
</release>

<release id="gnome-office">
  <name>Gnome-Office Productivity Applications</name>
  <module id="abiword"/>
  <module id="criawips"/>
  <module id="dia"/>
  <module id="gnumeric"/>
</release>

<release id="gnome-extras">
  <name>Extra Gnome Applications</name>
  <module id="rhythmbox"/>
</release>

