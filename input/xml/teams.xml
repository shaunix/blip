    <item>/list/gnome.org/gnome-i18n</item>
  </lists>
  <coordinators>
    <item>/person/gnome.org/danilo</item>
    <item>/person/gnome.org/menthos</item>
  </coordinators>
</team>

<group>
<defaults>
  <team>
    <ident>/team/gnome.org/i18n-%(id)</ident>
    <languages>
      <item>%(id)</item>
    </languages>
    <memberof>/team/gnome.org/i18n</memberof>
    <bug_type>bugzilla</bug_type>
    <bug_server idref="http://bugzilla.gnome.org/"/>
    <bug_product>l10n</bug_product>
  </team>
</defaults>

