</defaults>

<team id="documentation">
  <name>Documentation Team</name>
  <web>http://live.gnome.org/DocumentationProject</web>
  <alias>/team/gnome.bugs/gnome-user-docs-maint</alias>
  <lists>
    <item>/list/gnome.org/gnome-doc-list</item>
  </lists>
  <coordinators>
    <item>/person/gnome.org/shaunm</item>
  </coordinators>
</team>

<team id="usability">
  <name>Usability Team</name>
