<set id="mobile">
  <name>Mobile and Embedded</name>
  <set id="hildon">
    <name>Hildon</name>
    <jhbuild_scm_type>svn</jhbuild_scm_type>
    <jhbuild_scm_server>https://stage.maemo.org/svn/maemo/projects/haf/</jhbuild_scm_server>
    <jhbuild_scm_module>jhbuild_modules</jhbuild_scm_module>
    <jhbuild_scm_path>jhbuild_modules</jhbuild_scm_path>
    <jhbuild_scm_dir></jhbuild_scm_dir>
    <jhbuild_scm_file>hildon-desktop.modules</jhbuild_scm_file>
  </set>
  <set id="maemo">
    <name>Maemo</name>
    <jhbuild_scm_type>svn</jhbuild_scm_type>
    <jhbuild_scm_server>https://stage.maemo.org/svn/maemo/projects/haf/</jhbuild_scm_server>
    <jhbuild_scm_module>jhbuild_modules</jhbuild_scm_module>
    <jhbuild_scm_path>jhbuild_modules</jhbuild_scm_path>
    <jhbuild_scm_dir></jhbuild_scm_dir>
    <jhbuild_scm_file>maemo.modules</jhbuild_scm_file>
  </set>
  <set id="matchbox">
    <name>Matchbox</name>
    <jhbuild_scm_file>mobile-2.24.modules</jhbuild_scm_file>
    <jhbuild_metamodule>meta-matchbox</jhbuild_metamodule>
  </set>
  <set id="pimlico">
    <name>Pimlico</name>
    <jhbuild_scm_file>mobile-2.24.modules</jhbuild_scm_file>
    <jhbuild_metamodule>meta-pimlico</jhbuild_metamodule>
  </set>
</set>

<group>
  <defaults>
    <module>
      <scm_type>svn</scm_type>
      <scm_server>http://svn.gnome.org/svn/</scm_server>
      <scm_module>%(id)</scm_module>
    </module>
  </defaults>
  <set id="gnome-extras">
    <name>Gnome Extras</name>
    <module id="banshee">
      <scm_branch>trunk</scm_branch>
      <scm_path>banshee/trunk/banshee</scm_path>
    </module>
    <module id="blam"/>
    <module id="conduit"/>
    <module id="devilspie"/>
    <module id="eog-plugins"/>
    <module id="empathy"/>
    <module id="epiphany-extensions"/>
    <module id="f-spot"/>
    <module id="gedit-plugins"/>
    <module id="gimp"/>
    <module id="gimp-data-extras"/>
    <module id="gimp-freetype"/>
    <module id="gimp-gap"/>
    <module id="gimp-help-2"/>
    <module id="gimp-perl"/>
    <module id="gimp-plugin-template"/>
    <module id="gimp-ruby"/>
    <module id="gimp-tiny-fu"/>
    <module id="gnumeric"/>
    <module id="gthumb"/>
    <module id="xchat-gnome"/>
  </set>
  <set id="gnome-infrastructure">
    <name>Gnome Infrastructure</name>
    <module id="bugzilla.gnome.org"/>
    <module id="damned-lies"/>
    <module id="library-web"/>
    <module id="release-notes">
      <scm_branch>gnome-2-22</scm_branch>
    </module>
    <module id="pulse">
      <scm_type>git</scm_type>
      <scm_server>http://www.gnome.org/~shaunm/git/</scm_server>
      <scm_path>pulse.git</scm_path>
    </module>
    <module id="svn-web"/>
  </set>
</group>

</group>
